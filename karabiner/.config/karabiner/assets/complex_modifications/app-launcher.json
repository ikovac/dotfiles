{
  "title": "Launch apps",
  "rules": [
    {
      "description": "Launch apps by right command+letters.",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Brave Browser.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Visual Studio Code.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Fork.app'"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["caps_lock"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Ghostty.app'"
            }
          ]
        },
        {
          "type": "basic",
